<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_customer_complaint" model="ir.sequence">
            <field name="name">Customer Complaint</field>
            <field name="code">customer.complaint</field>
            <field name="prefix">COMPLAINT/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_courier_booking" model="ir.sequence">
            <field name="name">Courier Booking</field>
            <field name="code">courier.booking</field>
            <field name="prefix">COURIERID/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
    

</odoo>
